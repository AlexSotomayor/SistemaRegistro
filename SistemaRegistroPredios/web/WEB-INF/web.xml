<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>IngresoPredio</servlet-name>
        <servlet-class>cl.aiep.controller.IngresoPredio</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GuardarCarpetaPredial</servlet-name>
        <servlet-class>cl.aiep.controller.GuardarCarpetaPredial</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GuardarDatosPredio</servlet-name>
        <servlet-class>cl.aiep.controller.GuardarDatosPredio</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GuardarDatosPropietarios</servlet-name>
        <servlet-class>cl.aiep.controller.GuardarDatosPropietarios</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GuardarUsuario</servlet-name>
        <servlet-class>cl.aiep.controller.GuardarUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdatePredio</servlet-name>
        <servlet-class>cl.aiep.controller.UpdatePredio</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeletePredio</servlet-name>
        <servlet-class>cl.aiep.controller.DeletePredio</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeletePropietario</servlet-name>
        <servlet-class>cl.aiep.controller.DeletePropietario</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>IngresoPredio</servlet-name>
        <url-pattern>/IngresoPredio</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GuardarCarpetaPredial</servlet-name>
        <url-pattern>/GuardarCarpetaPredial</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GuardarDatosPredio</servlet-name>
        <url-pattern>/GuardarDatosPredio</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GuardarDatosPropietarios</servlet-name>
        <url-pattern>/GuardarDatosPropietarios</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GuardarUsuario</servlet-name>
        <url-pattern>/GuardarUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdatePredio</servlet-name>
        <url-pattern>/UpdatePredio</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeletePredio</servlet-name>
        <url-pattern>/DeletePredio</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeletePropietario</servlet-name>
        <url-pattern>/DeletePropietario</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
